{
    "name": "clever-online-store-ui",
    "version": "0.1.3",
    "private": true,
    "scripts": {
        "start": "run-s codegen:graphql dev",
        "dev": "next -p 7777",
        "build": "next build",
        "serve": "next start",
        "codegen:graphql": "gql-gen -r dotenv/config --config=graphql-codegen.yml",
        "check-all": "run-p check:*",
        "check:ts": "tsc --project ./tsconfig.json",
        "check:eslint": "eslint --max-warnings 10 ./src",
        "check:stylelint": "stylelint ./src",
        "prettify": "prettier-eslint --write \"$PWD/src/**/*.{ts,tsx}\"",
        "release": "release-it --ci"
    },
    "dependencies": {
        "@apollo/client": "3.5.7",
        "@hookform/resolvers": "2.8.5",
        "@stripe/react-stripe-js": "1.7.0",
        "@stripe/stripe-js": "1.22.0",
        "apollo-upload-client": "17.0.0",
        "debug": "4.3.3",
        "downshift": "6.1.7",
        "graphql": "16.2.0",
        "graphql-upload": "13.0.0",
        "lodash.debounce": "4.0.8",
        "next": "12.0.8",
        "nprogress": "0.2.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "7.23.0",
        "styled-components": "5.3.6",
        "waait": "1.0.5",
        "webpack-merge": "5.8.0",
        "yup": "0.32.11",
        "zx": "7.0.8"
