{
    "name": "clever-online-store-ui",
    "version": "0.1.3",
    "private": true,
    "scripts": {
        "start": "run-s codegen:graphql dev",
        "dev": "next -p 7777",
        "build": "next build",
        "serve": "next start",
        "codegen:graphql": "gql-gen -r dotenv/config --config=graphql-codegen.yml",
        "check-all": "run-p check:*",
        "check:ts": "tsc --project ./tsconfig.json",
        "check:eslint": "eslint --max-warnings 10 ./src",
        "check:stylelint": "stylelint ./src",
        "prettify": "prettier-eslint --write \"$PWD/src/**/*.{ts,tsx}\"",
        "prepare": "husky install",
        "release": "release-it --ci"
    },
    "dependencies": {
        "@apollo/client": "3.5.6",
        "@hookform/resolvers": "2.8.5",
        "@stripe/react-stripe-js": "1.6.0",
        "@stripe/stripe-js": "1.21.1",
        "apollo-upload-client": "16.0.0",
        "debug": "4.3.3",
        "downshift": "6.1.7",
        "graphql": "16.2.0",
        "graphql-upload": "12.0.0",
        "lodash.debounce": "4.0.8",
        "next": "12.0.7",
        "nprogress": "0.2.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-hook-form": "7.22.4",
        "styled-components": "5.3.3",
        "waait": "1.0.5",
        "webpack-merge": "5.8.0",
        "yup": "0.32.11",
        "zx": "4.2.0"
    },
    "devDependencies": {
        "@babel/preset-env": "7.16.5",
        "@graphql-codegen/cli": "2.3.0",
        "@graphql-codegen/import-types-preset": "2.1.8",
        "@graphql-codegen/typescript": "2.4.1",
        "@graphql-codegen/typescript-apollo-client-helpers": "2.1.8",
        "@graphql-codegen/typescript-operations": "2.2.1",
        "@graphql-codegen/typescript-react-apollo": "3.2.2",
        "@next/bundle-analyzer": "12.0.7",
        "@types/apollo-upload-client": "14.1.0",
        "@types/debug": "4.1.7",
        "@types/lodash.debounce": "4.0.6",
        "@types/nprogress": "0.2.0",
        "@types/react": "17.0.38",
        "@types/styled-components": "5.1.18",
        "@typescript-eslint/eslint-plugin": "5.8.0",
        "@typescript-eslint/parser": "5.8.0",
        "auto-changelog": "2.3.0",
        "babel-plugin-styled-components": "2.0.2",
        "babel-plugin-transform-define": "2.0.1",
        "dotenv": "10.0.0",
        "eslint": "8.5.0",
        "eslint-config-dva": "0.0.16",
        "graphql-codegen-apollo-next-ssr": "1.7.1",
        "husky": "7.0.4",
        "npm-run-all": "4.1.5",
        "prettier": "2.5.1",
        "prettier-eslint-cli": "5.0.1",
        "release-it": "14.11.8",
        "stylelint": "14.2.0",
        "stylelint-config-recess-order": "3.0.0",
        "stylelint-config-standard": "24.0.0",
        "stylelint-config-styled-components": "0.1.1",
        "stylelint-config-styled-components-dva": "0.0.2",
        "stylelint-declaration-block-no-ignored-properties": "2.5.0",
        "stylelint-order": "5.0.0",
        "stylelint-processor-styled-components": "1.10.0",
