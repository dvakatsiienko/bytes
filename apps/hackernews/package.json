{
    "name": "hackernews",
    "version": "0.10.7",
    "scripts": {
        "start": "next",
        "restart": "run-s build:clean start",
        "build": "next build",
        "build:clean": "rimraf .next",
        "serve": "next start",
        "lint": "TIMING=1 eslint .",
        "typecheck": "tsc",
        "validate": "run-p lint typecheck",
        "codegen:graphql": "gql-gen -r dotenv/config --config \"./graphql-codegen.yml\"",
        "release:patch": "release-it --ci",
        "release:minor": "release-it --ci minor",
        "release:major": "release-it --ci major",
        "packages:install": "yarn",
        "packages:reinstall": "run-s build:clean packages:clean packages:install",
        "packages:clean": "rimraf node_modules yarn.lock",
        "seed": "dotenv -e .env.local run-s prisma:migrate:reset prisma:db:seed",
        "prisma:generate": "prisma generate",
        "prisma:migrate:reset": "prisma db push --force-reset",
        "prisma:db:seed": "prisma db seed",
        "prisma:studio": "dotenv -e .env.local -- prisma studio",
        "postinstall": "prisma generate"
    },
    "prisma": {
        "seed": "node -r @swc-node/register prisma/seed"
    },
    "dependencies": {
        "@hookform/resolvers": "3.1.0",
        "@next-auth/prisma-adapter": "1.0.6",
        "@nextui-org/react": "1.0.0-beta.12",
        "@prisma/client": "4.13.0",
        "@reduxjs/toolkit": "1.9.5",
        "@styled-icons/feather": "10.47.0",
        "@styled-icons/ionicons-sharp": "10.46.0",
        "@styled-icons/styled-icon": "10.7.0",
        "@tanstack/react-query": "4.29.5",
        "@tanstack/react-query-devtools": "4.29.6",
        "@trpc/client": "10.25.0",
        "@trpc/next": "10.25.0",
        "@trpc/react-query": "10.25.0",
        "@trpc/server": "10.25.0",
        "@vercel/analytics": "1.0.1",
        "autoprefixer": "10.4.14",
        "bcrypt": "5.1.0",
        "classnames": "2.3.2",
        "cookies": "0.8.0",
        "next": "13.4.1",
        "next-auth": "4.22.1",
        "next-themes": "0.2.1",
        "postcss": "8.4.23",
        "prettier-plugin-tailwindcss": "0.2.8",
        "prisma": "4.13.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.43.9",
        "react-redux": "8.0.5",
        "reset-css": "5.0.1",
        "sass": "1.62.1",
        "styled-components": "5.3.10",
        "superjson": "1.12.3",
        "tailwindcss": "3.3.2",
        "waait": "1.0.5",
        "zod": "3.21.4"
    },
    "devDependencies": {
        "@swc-node/register": "1.6.5",
        "@swc/core": "1.3.57",
        "@types/bcrypt": "5.0.0",
        "@types/cookies": "0.7.7",
        "@types/node": "20.1.1",
        "@types/react": "18.2.6",
        "@types/react-dom": "18.2.4",
        "@types/redux-logger": "3.0.9",
        "@types/styled-components": "5.1.26",
        "dotenv-cli": "7.2.1",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.8",
        "prettier-eslint-cli": "7.1.0",
        "redux-logger": "3.0.6",
        "rimraf": "5.0.0"
    }
}
