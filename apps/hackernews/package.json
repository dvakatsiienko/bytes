{
    "name": "hackernews",
    "version": "0.10.7",
    "scripts": {
        "start": "next",
        "restart": "run-s build:clean start",
        "build": "next build",
        "build:clean": "rimraf .next",
        "serve": "next start",
        "lint": "TIMING=1 eslint .",
        "typecheck": "tsc",
        "validate": "run-p lint typecheck",
        "codegen:graphql": "gql-gen -r dotenv/config --config \"./graphql-codegen.yml\"",
        "release:patch": "release-it --ci",
        "release:minor": "release-it --ci minor",
        "release:major": "release-it --ci major",
        "packages:install": "yarn",
        "packages:reinstall": "run-s build:clean packages:clean packages:install",
        "packages:clean": "rimraf node_modules yarn.lock",
        "seed": "dotenv -e .env.local run-s prisma:migrate:reset prisma:db:seed",
        "prisma:generate": "prisma generate",
        "prisma:migrate:reset": "prisma db push --force-reset",
        "prisma:db:seed": "prisma db seed",
        "prisma:studio": "dotenv -e .env.local -- prisma studio",
        "postinstall": "prisma generate"
    },
    "prisma": {
        "seed": "node -r @swc-node/register prisma/seed"
    },
    "dependencies": {
        "@hookform/resolvers": "3.3.1",
        "@next-auth/prisma-adapter": "1.0.7",
        "@nextui-org/react": "1.0.0-beta.13",
        "@prisma/client": "4.15.0",
        "@reduxjs/toolkit": "1.9.5",
        "@styled-icons/feather": "10.47.0",
        "@styled-icons/ionicons-sharp": "10.46.0",
        "@styled-icons/styled-icon": "10.7.0",
        "@tanstack/react-query": "4.35.0",
        "@tanstack/react-query-devtools": "4.35.0",
        "@trpc/client": "10.38.1",
        "@trpc/next": "10.38.1",
        "@trpc/react-query": "10.38.1",
        "@trpc/server": "10.38.1",
        "@vercel/analytics": "1.0.2",
        "autoprefixer": "10.4.15",
        "bcrypt": "5.1.1",
        "classnames": "2.3.2",
        "cookies": "0.8.0",
        "next": "13.4.19",
        "next-auth": "4.23.1",
        "next-themes": "0.2.1",
        "postcss": "8.4.29",
        "prisma": "4.15.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.46.1",
        "react-redux": "8.1.2",
        "reset-css": "5.0.2",
        "sass": "1.66.1",
        "styled-components": "5.3.11",
        "superjson": "1.13.1",
        "tailwindcss": "3.3.3",
        "typescript": "5.2.2",
        "waait": "1.0.5",
        "zod": "3.22.2"
    },
    "devDependencies": {
        "@swc-node/register": "1.6.7",
        "@swc/core": "1.3.83",
        "@types/bcrypt": "5.0.0",
        "@types/cookies": "0.7.8",
        "@types/node": "20.6.0",
        "@types/react": "18.2.21",
        "@types/react-dom": "18.2.7",
        "@types/redux-logger": "3.0.9",
        "@types/styled-components": "5.1.27",
        "dotenv-cli": "7.3.0",
        "npm-run-all": "4.1.5",
        "prettier": "2.8.8",
        "redux-logger": "3.0.6",
        "rimraf": "5.0.1"
    }
}
