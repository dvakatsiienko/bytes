{
    "name": "hackernews",
    "version": "0.10.7",
    "scripts": {
        "start": "next",
        "restart": "run-s build:clean start",
        "build": "next build",
        "build:clean": "rimraf .next",
        "serve": "next start",
        "lint": "TIMING=1 eslint .",
        "typecheck": "tsc",
        "validate": "run-p lint typecheck",
        "release:patch": "release-it --ci",
        "release:minor": "release-it --ci minor",
        "release:major": "release-it --ci major",
        "packages:install": "yarn",
        "packages:reinstall": "run-s build:clean packages:clean packages:install",
        "packages:clean": "rimraf node_modules yarn.lock",
        "seed": "dotenv -e .env.local run-s prisma:migrate:reset prisma:db:seed",
        "prisma:generate": "prisma generate",
        "prisma:migrate:reset": "prisma db push --force-reset",
        "prisma:db:seed": "prisma db seed",
        "prisma:studio": "dotenv -e .env.local -- prisma studio",
        "postinstall": "prisma generate"
    },
    "prisma": {
        "seed": "node -r @swc-node/register prisma/seed"
    },
    "dependencies": {
        "@hookform/resolvers": "3.3.2",
        "@next-auth/prisma-adapter": "1.0.7",
        "@nextui-org/react": "2.1.12",
        "@prisma/client": "5.3.0",
        "@reduxjs/toolkit": "1.9.7",
        "@styled-icons/feather": "10.47.0",
        "@styled-icons/ionicons-sharp": "10.46.0",
        "@styled-icons/styled-icon": "10.7.0",
        "@tanstack/react-query": "4.35.3",
        "@tanstack/react-query-devtools": "4.35.3",
        "@trpc/client": "10.38.2",
        "@trpc/next": "10.38.2",
        "@trpc/react-query": "10.38.2",
        "@trpc/server": "10.38.2",
        "@vercel/analytics": "1.1.1",
        "bcrypt": "5.1.1",
        "classnames": "2.5.1",
        "cookies": "0.8.0",
        "framer-motion": "10.16.16",
        "next": "13.4.19",
        "next-auth": "4.23.1",
        "prisma": "5.3.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.48.2",
        "react-redux": "8.1.3",
        "sass": "1.69.6",
        "styled-components": "6.1.6",
        "superjson": "1.13.1",
        "waait": "1.0.5",
        "zod": "3.22.4"
    },
    "devDependencies": {
        "@swc-node/register": "1.6.8",
        "@swc/core": "1.3.102",
        "@types/bcrypt": "5.0.2",
        "@types/cookies": "0.7.10",
        "@types/node": "22.10.10",
        "@types/react": "19.0.7",
        "@types/react-dom": "19.0.3",
        "@types/redux-logger": "3.0.13",
        "autoprefixer": "10.4.16",
        "dotenv-cli": "7.3.0",
        "npm-run-all": "4.1.5",
        "postcss": "8.4.32",
        "prettier": "3.4.2",
        "redux-logger": "3.0.6",
        "rimraf": "5.0.5",
        "tailwindcss": "3.3.5",
        "typescript": "5.2.2"
    }
}
