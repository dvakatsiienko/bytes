{
    "name": "hackernews",
    "version": "0.10.7",
    "scripts": {
        "start": "NODE_OPTIONS='--inspect' next dev",
        "restart": "run-s build:clean start",
        "build": "next build",
        "build:clean": "rimraf .next",
        "serve": "next start",
        "lint": "eslint .",
        "typecheck": "tsc",
        "validate": "run-p lint typecheck",
        "release:patch": "release-it --ci",
        "release:minor": "release-it --ci minor",
        "release:major": "release-it --ci major",
        "packages:install": "yarn",
        "packages:reinstall": "run-s build:clean packages:clean packages:install",
        "packages:clean": "rimraf node_modules yarn.lock",
        "seed": "dotenv -e .env.local run-s prisma:migrate:reset prisma:db:seed",
        "prisma:generate": "prisma generate",
        "prisma:migrate:reset": "prisma db push --force-reset",
        "prisma:db:seed": "prisma db seed",
        "prisma:studio": "dotenv -e .env.local -- prisma studio",
        "postinstall": "prisma generate"
    },
    "prisma": {
        "seed": "node -r @swc-node/register prisma/seed"
    },
    "dependencies": {
        "@hookform/resolvers": "4.1.3",
        "@next-auth/prisma-adapter": "1.0.7",
        "@nextui-org/react": "2.1.12",
        "@prisma/client": "6.5.0",
        "@styled-icons/feather": "10.47.0",
        "@styled-icons/ionicons-sharp": "10.46.0",
        "@styled-icons/styled-icon": "10.7.1",
        "@tanstack/react-query": "5.69.0",
        "@tanstack/react-query-devtools": "5.69.0",
        "@trpc/client": "10.38.2",
        "@trpc/next": "10.38.2",
        "@trpc/react-query": "10.38.2",
        "@trpc/server": "10.38.2",
        "bcrypt": "5.1.1",
        "classnames": "2.5.1",
        "cookies": "0.8.0",
        "next": "15.2.4",
        "next-auth": "4.24.11",
        "prisma": "6.5.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-hook-form": "7.54.2",
        "sass": "1.85.0",
        "styled-components": "6.1.15",
        "superjson": "1.13.1",
        "waait": "1.0.5",
        "zod": "3.24.2"
    },
    "devDependencies": {
        "@swc-node/register": "1.10.10",
        "@swc/core": "1.11.13",
        "@types/bcrypt": "5.0.2",
        "@types/cookies": "0.7.10",
        "@types/node": "22.13.14",
        "@types/react": "19.0.12",
        "@types/react-dom": "19.0.4",
        "autoprefixer": "10.4.21",
        "dotenv-cli": "7.3.0",
        "npm-run-all": "4.1.5",
        "postcss": "8.5.3",
        "prettier": "3.5.3",
        "rimraf": "6.0.1",
        "tailwindcss": "4.0.17",
        "typescript": "5.8.2"
    }
}
