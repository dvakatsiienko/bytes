{
    "name": "next-launchpad",
    "version": "0.1.0",
    "private": true,
    "author": {
        "name": "Dima Vakatsiienko",
        "email": "imagnum.satellite@gmail.com"
    },
    "scripts": {
        "start": "next",
        "build": "next build",
        "build:analyze": "ANALYZE=true next build",
        "serve": "next start",
        "codegen:graphql": "gql-gen -r dotenv/config --config \"./graphql-codegen.yml\"",
        "check-all": "run-p check:*",
        "check:ts": "tsc --project ./tsconfig.json",
        "check:eslint": "eslint .",
        "check:stylelint": "stylelint ./src",
        "prettify": "prettier-eslint --write \"$PWD/src/**/*.{ts,tsx}\"",
        "x-prepare": "husky install"
    },
    "dependencies": {
        "@apollo/client": "3.5.6",
        "@reduxjs/toolkit": "1.7.1",
        "apollo-link-logger": "2.0.0",
        "debug": "4.3.3",
        "deepmerge": "4.2.2",
        "graphql": "16.2.0",
        "lodash": "4.17.21",
        "next": "12.0.7",
        "nprogress": "0.2.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-redux": "7.2.6",
        "sanitize.css": "13.0.0",
        "styled-components": "5.3.3"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "2.3.1",
        "@graphql-codegen/typescript": "2.4.2",
        "@graphql-codegen/typescript-apollo-client-helpers": "2.1.9",
