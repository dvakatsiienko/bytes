{
    "name": "x-com-chat",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "dev:convex": "convex dev",
        "build": "next build --turbopack",
        "build:analyze": "ANALYZE=true next build",
        "build:clean": "rimraf .next",
        "serve": "next start",
        "lint": "eslint",
        "lint:inspect": "pnpx @eslint/config-inspector@latest",
        "typecheck": "tsc",
        "check": "turbo lint typecheck",
        "db:push": "prisma db push",
        "db:generate": "prisma generate --no-engine",
        "db:reset": "prisma db push --force-reset",
        "db:seed": "prisma db seed",
        "db:reinit": "pnpm db:reset && pnpm db:generate && pnpm db:seed",
        "postinstall": "prisma generate --no-engine",
        "knip": "knip"
    },
    "dependencies": {
        "@ai-sdk/groq": "1.2.9",
        "@ai-sdk/react": "1.2.12",
        "@clerk/nextjs": "6.20.0",
        "@clerk/themes": "2.2.46",
        "@next/bundle-analyzer": "15.3.2",
        "@openrouter/ai-sdk-provider": "0.4.6",
        "@prisma/client": "6.8.2",
        "@prisma/extension-accelerate": "2.0.0",
        "@radix-ui/react-dialog": "1.1.14",
        "@radix-ui/react-dropdown-menu": "2.1.15",
        "@radix-ui/react-icons": "1.3.2",
        "@radix-ui/react-select": "2.2.5",
        "@radix-ui/react-separator": "1.1.7",
        "@radix-ui/react-slot": "1.2.3",
        "@radix-ui/react-toggle": "1.1.9",
        "@radix-ui/react-toggle-group": "1.1.10",
        "@radix-ui/react-tooltip": "1.2.7",
        "@tailwindcss/forms": "0.5.10",
        "@tailwindcss/typography": "0.5.16",
        "@tanstack/react-query": "5.77.0",
        "@use-it/event-listener": "0.1.7",
        "ai": "4.3.16",
        "babel-plugin-react-compiler": "19.1.0-rc.2",
        "convex": "1.24.1",
        "cva": "1.0.0-beta.3",
        "fast-deep-equal": "3.1.3",
        "jotai": "2.12.4",
        "lucide-react": "0.511.0",
        "motion": "12.12.1",
        "next": "15.3.2",
        "next-themes": "0.4.6",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-markdown": "10.1.0",
        "react-scan": "0.3.4",
        "remark-gfm": "4.0.1",
        "server-only": "0.0.1",
        "tailwind-merge": "3.3.0",
        "tailwind-scrollbar": "4.0.2",
        "tw-animate-css": "1.3.0",
        "waait": "1.0.5"
    },
    "devDependencies": {
        "@swc-node/register": "1.10.10",
        "@tailwindcss/postcss": "4.1.7",
        "@tanstack/react-query-devtools": "5.77.0",
        "@types/node": "22.15.21",
        "@types/react": "19.1.5",
        "@types/react-dom": "19.1.5",
        "jotai-devtools": "0.12.0",
        "knip": "5.58.0",
        "prettier": "3.5.3",
        "prisma": "6.8.2",
        "tailwindcss": "4.1.7",
        "typescript": "5.8.3"
    },
    "prisma": {
        "seed": "node -r @swc-node/register prisma/seed/init"
    }
