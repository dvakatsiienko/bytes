{
    "name": "turborepo",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev --parallel",
        "start:hackernews": "turbo run start --filter='@hackernews/*'",
        "start:hackernews-api": "turbo run start --filter=@hackernews/api",
        "build:hackernews-ui": "turbo run build --filter='@hackernews/ui'",
        "start:basic": "turbo run start --filter='@basic/*'",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "packages:reinstall": "rimraf **/**/node_modules && pnpm install --fix-lockfile"
    },
    "devDependencies": {
        "eslint": "8.39.0",
        "prettier": "2.7.1",
        "rimraf": "5.0.0",
        "turbo": "1.9.3",
        "eslint-config-dva": "0.0.16",
        "eslint-config-core": "workspace:*",
        "tsconfig-core": "workspace:*"
    },
    "engines": {
        "pnpm": ">=8.0.0",
        "node": ">=18.12.0"
    }
}
