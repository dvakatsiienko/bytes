{
    "name": "turborepo",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start:basic": "turbo run start --filter='@basic/*'",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "packages:reinstall": "rimraf **/{node_modules,.next} && pnpm install --force"
    },
    "devDependencies": {
        "eslint": "8.39.0",
        "eslint-config-core": "workspace:*",
        "eslint-config-dva": "0.0.16",
        "prettier": "2.8.8",
        "rimraf": "5.0.0",
        "tsconfig-core": "workspace:*",
        "turbo": "1.9.3"
    },
    "engines": {
        "pnpm": ">=8.0.0",
        "node": ">=18.12.0"
    }
}
